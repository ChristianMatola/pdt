         &lt;p&gt;
&lt;pre&gt;
&lt;extension point=&quot;org.eclipse.php.ui.phpContentAssistProcessor&quot;&gt;
    &lt;processor
        class=&quot;test.MyContentAssistProcessor&quot;
        id=&quot;test.MyContentAssistProcessor&quot;
        name=&quot;My PHP Content Assist Processor&quot;/&gt;
   &lt;/extension&gt;
&lt;/pre&gt;
&lt;/p&gt;
      </documentation>
   </annotation>

   <annotation>
